<!DOCTYPE html>
<html>
<head>
  <title>Emapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= render partial: "layouts/header" %>

    <div class="container">
        <div class="row">
            <div class="col-md-10">
                 <%= yield %>
            </div>
            <div class="col-md-2">
                <%= render partial: "events/tag_cloud"%>
            </div>
        </div>
    </div>
</body>
</html>
